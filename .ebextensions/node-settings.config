option_settings:
  - namespace: aws:elasticbeanstalk:container:nodejs
    option_name: NodeCommand
    value: "npm run deploy"
  - namespace: aws:elasticbeanstalk:container:nodejs 
    option_name: NodeVersion
    value: 6.10.0
  - namespace: aws:elasticbeanstalk:container:nodejs 
    option_name: ProxyServer
    value: nginx
  - namespace: aws:elasticbeanstalk:container:nodejs 
    option_name: GzipCompression
    value: true